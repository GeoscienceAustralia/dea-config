"template": "<table>  <tr><td><b>Time</b></td><td>{{time}}</td></tr>  <tr><td><b>Blue - 480</b></td><td>{{bands.nbart_blue}}</td></tr>  <tr><td><b>Green - 560</b></td><td>{{bands.nbart_green}}</td></tr>  <tr><td><b>Red - 660</b></td><td>{{bands.nbart_red}}</td></tr>  <tr><td><b>Near Infrared (NIR) - 870</b></td><td>{{bands.nbart_nir}}</td></tr>  <tr><td><b>Shortwave Infrared (SWIR) - 1610</b></td><td>{{bands.nbart_swir_1}}</td></tr>  <tr><td><b>Shortwave Infrared (SWIR) - 2220</b></td><td>{{bands.nbart_swir_2}}</td></tr>  <tr><td><b>Normalised Difference Vegetation Index</b></td><td>{{bands.ndvi}}</td></tr>  <tr><td><b>Normalised Difference Water Index</b></td><td>{{bands.ndwi}}</td></tr><tr><td><b>Modified Normalised Difference Water Index</b></td><td>{{bands.mndwi}}</td></tr><tr><td><b>Normalised Burn Ratio</b></td><td>{{bands.nbr}}</td></tr></table><p><chart id='{{x}}{{y}}{{time}}' title='NBART at {{x}},{{y}}' column-units='Wavelength (nm), Reflectance x 10k' preview-x-label='NBART Reflectance'>nm,NBART Reflectance\n480,{{bands.nbart_blue}}\n560,{{bands.nbart_green}}\n660,{{bands.nbart_red}}\n870,{{bands.nbart_nir}}\n1610,{{bands.nbart_swir_1}}\n2220,{{bands.nbart_swir_2}}</chart></p><p><b>Imagery available for dates:</b>{{#data_available_for_dates}}<br/>{{.}}{{/data_available_for_dates}}</p>"